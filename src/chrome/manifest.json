{
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [ "js/contentscript.js" ],
      "matches": [ "<all_urls>" ],
      "exclude_matches": [ "*://remotedesktop.google.com/*" ],
      "run_at": "document_start"
    }
  ],
  "description": "Mouse Gestures.",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 3,
  "minimum_chrome_version": "88",
  "author": "HDBENCH.NET",
  "name": "KT Mouse Gestures",
  "permissions": [ "activeTab" ],
  "version": "24.11.10.0"
}
